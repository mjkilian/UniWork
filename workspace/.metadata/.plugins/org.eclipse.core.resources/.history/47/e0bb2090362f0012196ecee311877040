package Calendar.Event;
import Calendar.Category.*;

public class Event {
	private String title;
	private EventTime time;
	Category category;	
	
	public Event(String title, EventTime time) {
		super();
		this.title = title;
		this.time = time;
	}
	
	public Event(String title, EventTime time, Category category) {
		super();
		this.title = title;
		this.time = time;
		this.category = category;
	}
	
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public EventTime getTime() {
		return time;
	}
	public void setTime(EventTime time) {
		this.time = time;
	}
	
	/*Rest of attributs e.g description location etc*/
	
	
	
}
