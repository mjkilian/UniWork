package Calendar.Event;
import Calendar.Category.*;

public class Event {
	private String title;
	private EventTime starttime, endtime;
	private Category category = null ;	
	
	public Event(String title, EventTime stime, EventTime etime) {
		super();
		this.title = title;
		this.starttime = stime;
		this.endtime = etime;
	}
	
	public Event(String title, EventTime stime,EventTime etime, Category category) {
		super();
		this.title = title;
		this.starttime = stime;
		this.endtime = etime;
		this.category = category;
	}
	
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public EventTime getTime() {
		return time;
	}
	public void setTime(EventTime time) {
		this.time = time;
	}

	public Category getCategory() {
		return category;
	}

	public void setCategory(Category category) {
		this.category = category;
	}
	
	/*Rest of attributs e.g description location etc*/
	
	
	
}
