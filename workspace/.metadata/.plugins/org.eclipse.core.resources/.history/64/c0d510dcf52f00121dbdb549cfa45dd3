package Calendar.Year;
import Calendar.Month.*;

public class Year {
	private int number;
	private Month months[] = new Month[12];
	private boolean isLeap;
	
	public Year(int number){
		this.number = number;
		//set leap year
		for(int i = 0; i < months.length; i++){
			months[i] = new Month(i, isLeap);
		}
	}
}
